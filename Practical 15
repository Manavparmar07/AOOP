
class C{
    public void p1()
    {
        System.out.println("this is C class");
    }
}
class E extends C{
    public void p2()
    {
        System.out.println("this is E class");
    }
}
class K extends E{
    public void p3()
    {
        System.out.println("this is K class");
    }
}
public class StudentAccount {
    public static void main(String[] args) {
        C obj=new C();
        obj.p1();
        obj.p2();
        obj.p3();
    }

}
