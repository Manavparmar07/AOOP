
 class Animals{
     public void Eating() {
         System.out.println("Eating...");
     }
 }
 class Dog extends Animals {
     public void Animalsname() {
         System.out.println("Dog...");
     }
 }

 public class Main{
         public static void main(String[] args) {
           Dog d1=new Dog();
             d1.Eating();
             d1.Animalsname();
         }
     }
