class O {
    public void f1(){
        System.out.println("this class O");
    }
}
class P extends O{
    public void f2(){
        System.out.println("this class P");
    }
}
class C extends O{
    public void f3(){
        System.out.println("this class C");
    }
}

public class Main{
    public static void main(String[] args) {
        P obj=new P();
        obj.f1();
        obj.f2();

        C obj2=new C();
        obj2.f1();
        obj2.f3();
    }
}
